/*loading*/

#loading {
    z-index: 999999;
    position: fixed;
    top: 0%;
    left: 0%;
    width: 100%;
    height: 100%;
    background-color: rgba(255, 255, 255, 0.5);
}

#loading .loading-cnt {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 180rpx;
    height: 180rpx;
    -webkit-transform: translate(-90rpx, -90rpx);
    transform: translate(-90rpx, -90rpx);
    z-index: 9999999;
}

#loading .loading-cnt image {
    width: 180rpx;
    height: 180rpx;
}

.bg-caitiao {
    width: 100%;
    height: 4rpx;
    background: #fcfcfc url('https://shgm.jjyyx.com/m/images/bg_caitiao.png') no-repeat center top;
    -webkit-background-size: 100% 4rpx;
    background-size: 100% 4rpx;
}

