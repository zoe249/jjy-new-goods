}

.group-draw-des .draw-des .line {
    position: absolute;
    top: 20rpx;
    right: 6rpx;
    height: 1rpx;
    background-color: #b9bebd;
    width: 24rpx;
}

.group-draw-des .draw-des1 {
    width: 224rpx;
}

.group-draw-des .draw-des2 {
    width: 214rpx;
}

.group-draw-des .draw-des3 {
    width: 200rpx;
}

.swiper-container {
    position: relative;
    width: 100%;
    height: auto;
}

.swiper-container .goods-imgcover-container {
    width: 750rpx;
    height: 750rpx;
    position: relative;
}

.swiper-container .indicator-container {
    position: absolute;
    left: 0;
    bottom: 20rpx;
    width: 750rpx;
    height: 40rpx;
    display: flex;
    justify-content: center;
    align-items: flex-end;
}

.indicator-container .indicator-item {
    width: 8rpx;
    height: 8rpx;
    margin: 0 4rpx;
    border-radius: 4rpx;
    transition: width 0.2s ease;
    background-color: #b9bebd;
}


