/* pages/goods/detail/detail.wxss */

page {
    color: #777;
    background-color: #fff;
}

.hidden {
    display: none !important;
}

.clearfix:after {
    clear: both;
}

.clearfix {
    zoom: 1;
}

.clearfix:before,
.clearfix:after {
    content: "";
    display: block;
    overflow: hidden;
    visibility: hidden;
    width: 0;
    height: 0;
}
.back-zhibo{
  z-index: 99;
  position: fixed;
  top:20rpx;
  left:0;
  background-color: #FF4752;
  color:#fff;
  line-height: 50rpx;
  height: 50rpx;
  padding:0 20rpx;
  border-radius: 0 25rpx 25rpx 0;
  font-size: 28rpx;
}
.wrapper {
    height: 100vh;
    padding-bottom: 112rpx;
}

.group-rule-des {
    text-align: center;
    line-height: 48rpx;
    color: var(--blue);
    font-size: 24rpx;
    background-color: #e6e9ed;
}

.back-home {
    background: #fff;
    z-index: 99;
    position: fixed;
    bottom: 200rpx;
    right: 30rpx;
    width: 96rpx;
    height: 96rpx;
    background: url("https://shgm.jjyyx.com/m/images/detail/icon-back-home.png") no-repeat center;
    background-size: 100% 100%;
    text-indent: -9999rpx;
}


/* 帮帮团头部 */

.help-group-head {
    padding: 0 32rpx;
}

.help-group-head .help-title-box {
    position: relative;
}

.help-group-head .help-title-box .help-title {
    font-size: 24rpx;
    color: #94969c;
}

.help-group-head .help-title-price {
    font-size: 32rpx;
    color: #FF4752;
    line-height: 44rpx;
    display: inline-block;
}

.help-group-head .help-title-people {
    display: inline-block;
}

.help-group-head .help-already {
    font-size: 24rpx;
    color: #777;
    line-height: 44rpx;
    position: absolute;
    right: 0;
    top: 4rpx;
}

.help-group-head .help-salePrice {
    padding: 20rpx 0;
    font-size: 24rpx;
    color: #94969c;
    line-height: 32rpx;
    text-decoration: line-through;
}

.help-group-head .help-tip {
    border-top: 1rpx solid #e7e7e7;
    min-height: 88rpx;
    position: relative;
    padding: 26rpx 0 26rpx 85rpx;
    font-size: 24rpx;
    line-height: 32rpx;
    color: #777;
}

.help-group-head .help-tip .help-tip-title {
    position: absolute;
    top: 24rpx;
    left: 0;
    color: #ffa601;
    border: 1rpx solid #ffa601;
    line-height: 32rpx;
    font-size: 24rpx;
    padding: 0 8rpx;
}


/* 抽奖团头部信息 */

.draw-head {
    padding: 0 0 20rpx;
}

.draw-tip-box {
    padding: 0 32rpx;
}

.draw-tip {
    border-top: 1rpx solid #e7e7e7;
    min-height: 88rpx;
    position: relative;
    padding: 26rpx 0 26rpx 85rpx;
    font-size: 24rpx;
    line-height: 32rpx;
    color: #777;
}

.draw-tip .draw-tip-title {
    position: absolute;
    top: 24rpx;
    left: 0;
    color: #ffa601;
    border: 1rpx solid #ffa601;
    line-height: 32rpx;
    font-size: 24rpx;
    padding: 0 8rpx;
}


/* 帮帮团规则玩法 */

.goods-promotion-container.help-promotion-container .group-header-container {
    padding: 0 32rpx;
}

.help-promotion-container .help-head {
    position: relative;
}

.help-promotion-container .help-head-left {
    height: 86rpx;
    font-size: 30rpx;
    line-height: 42rpx;
    color: #313031;
    padding: 22rpx 0;
}

.help-promotion-container .help-head-info {
    position: absolute;
    right: 0;
    top: 0;
    height: 85rpx;
    line-height: 85rpx;
    color: #94969c;
    font-size: 24rpx;
    padding-right: 27rpx;
}

.help-promotion-container .help-head-info::after {
    content: '';
    position: absolute;
    right: 0rpx;
    top: 50%;
    width: 16rpx;
    height: 16rpx;
    border-left: 2rpx solid #999;
    border-bottom: 2rpx solid #999;
    -webkit-transform: translateY(-50%) scaleY(1) rotateZ(-135deg);
    transform: translateY(-50%) scaleY(1) rotateZ(-135deg);
}

.help-promotion-container .group-help-des {
    border-top: 1rpx solid #e7e7e7;
    color: #313031;
    font-size: 24rpx;
    line-height: 96rpx;
}


/* 抽奖规则 */

.goods-promotion-container.draw-promotion-container .group-header-container {
    padding: 0 32rpx;
}

.draw-promotion-container .group-draw-head {
    position: relative;
}

.draw-promotion-container .draw-head-left {
    height: 86rpx;
    font-size: 30rpx;
    line-height: 42rpx;
    color: #313031;
    padding: 22rpx 0;
}

.draw-promotion-container .draw-head-info {
    position: absolute;
    right: 0;
    top: 0;
    height: 85rpx;
    line-height: 85rpx;
    color: #94969c;
    font-size: 24rpx;
    padding-right: 27rpx;
}

.draw-promotion-container .draw-head-info::after {
    content: '';
    position: absolute;
    right: 0rpx;
    top: 50%;
    width: 16rpx;
