<import src="/templates/global/global.wxml" />
<wxs module="filter" src="../../../utils/filter.wxs"></wxs>
<view
  class="container order-detail{{showNoData?' bg-white':''}}{{isIphoneX ? ' iphone-x-patcher-container' : ''}}{{hiddenMealNo?'':' overflowHidden'}}">
  <template is="modal" data="{{...modalData}}"></template>
  <template is="toast" data="{{...toastData}}"></template>
  <view id="loading" hidden="{{loadingHidden}}">
    <view class="loading-cnt">
      <image src="https://shgm.jjyyx.com/m/images/loadings.gif"></image>
    </view>
  </view>
  <block wx:if="{{showNoData}}">
    <template is="empty" data="{{emptyMsg:'暂无数据',errorImageName:'error_img5.png'}}"></template>
  </block>
  <block wx:else>
    <template is="orderDetail" data="{{...result}}"></template>
    <template is="orderDetailBtn" data="{{...result,reportSubmit:reportSubmit}}"></template>
  </block>
</view>

<block wx:if="{{showRedBagPop}}">
  <view id="pop-red-bag" catchtap="closeRedBagPop">
    <view class="pop-cnt">
      <view class="head">
        <image class="img" src="https://shgm.jjyyx.com/m/images/popRedBagBg.jpg"></image>
      </view>
      <view class="cnt">
        <!-- <view class="p1">{{redBagJson.nameTitle}}</view>
        <view class="p2">{{redBagJson.nameCnt}}</view> -->
        <view class="p3">{{redBagJson.name||'分享抢红包'}}</view>
        <view class="money-box">
          <!-- <image class="money-img" src="https://shgm.jjyyx.com/m/images/bgmax@2x.png"></image> -->
          <view class="money">
            <view class="span">¥</view>
            <view class="em">{{redBagJson.totalPrice}}</view>
          </view>
        </view>
        <button id="btn-pop-red" catchtap="shareRedBagPop" open-type="share"
          data-red-bag-json="{{redBagJson}}">抢最大红包</button>
      </view>
      <view id="pop-close" catchtap="closeRedBagPop"></view>
    </view>
  </view>
</block>
<block wx:if="{{showPhone}}">
  <view id="phone-pop">
    <view class="phone-cnt">
      <view class="phone-num">{{phoneNum}}</view>
      <view class="phone-btn">
        <view class="phone-cancel" catchtap="closePhonePop">取消</view>
        <view class="phone-confirm" catchtap="confirmPhone">呼叫</view>
      </view>
    </view>
  </view>
</block>
<template name="orderDetail">
  <block wx:if="{{isShareFlag&&isShareFlag==1}}">
    <view id="orderDetail-red-bag" catchtap="getRedBag" data-warehouse-id="{{warehouseId}}" data-shop-id="{{shopId}}"
      data-order-id="{{orderId}}">
      <image class="img" src="https://shgm.jjyyx.com/m/images/detailRedBagBtn.png"></image>
    </view>
  </block>
  <view id="order-detail-box" class="{{isShareFlag&&isShareFlag==1?'hasRedBagPadding':''}}">
    <block wx:if="{{orderStatus==48||orderStatus==116}}">
      <view class="order-buyer-cancel color-ff5c33">
        <icon></icon>
        <text>订单已取消</text>
        <block wx:if="{{cancelRemark}}">
          <view class="color-999 f-24">取消原因：{{cancelRemark}}</view>
        </block>
      </view>
    </block>
    <block wx:elif="{{orderStatus==115}}">
      <view class="order-buyer-cancel color-ff5c33">
        <icon></icon>
        <text>订单取消</text>
        <view class="color-999 f-24">取消原因：{{cancelRemark||'用户取消'}}</view>
      </view>
    </block>
    <block wx:elif="{{orderStatus==47||orderStatus==49}}">
      <view class="order-buyer-cancel color-ff5c33">
        <icon></icon>
        <text>订单已关闭</text>
        <block wx:if="{{cancelRemark}}">
          <view class="color-999 f-24">关闭原因：{{cancelRemark}}</view>
        </block>
      </view>
    </block>
    <block wx:elif="{{offlineOrderType==1184&&shippingTypes[0]!=113||offlineOrderType==1122&&shippingTypes[0]!=113}}">
      <view class="offlineOrderType-box">
        <view class="span">订单支付成功</view>
      </view>
    </block>
    <block wx:elif="{{orderStatus==51}}">
      <view class="order-detail-noPay" paytimeleft="{{payTimeLeft}}">
        <view class="div">订单即将失效，请立即付款</view>
        <view class="p">订单将于
          <text>{{$payTimeLeft}}</text>后自动关闭
        </view>
      </view>
    </block>
    <block wx:elif="{{isCardFlag==1}}">
      <!-- <view class="offlineOrderType-box">
                <view class="span">订单支付成功</view>
            </view>-->
    </block>
    <block
      wx:elif="{{goodsSence&&goodsSence>0&&(orderStateDiagrams&&orderStateDiagrams.length<1||!orderStateDiagrams)}}">
      <view class="offlineOrderType-box mb8">
        <view class="span">支付成功，可以在设备上操作啦！</view>
      </view>
    </block>
    <block wx:else>
      <block wx:if='{{orderStateDiagrams.length>2}}'>
        <view class='group-process-box group-process-box{{orderStateDiagrams.length}}'>
          <block wx:for="{{orderStateDiagrams}}" wx:for-item="progress" wx:key="orderStateDiagrams{{i}}"
            wx:for-index="i">
            <view
              class="group-process-li group-process-li{{i}}{{progress.nodeStatus?' on':''}}{{(orderStateDiagrams.length-1)==i?' last':''}}">
              <view class='group-process-text'>
                {{(!progress.lineDownMsg&&i==orderStateDiagrams.length-1)?'已完成':progress.lineDownMsg}}</view>
              <block wx:if="{{progress.lineUpMsg}}">
                <view class="group-process-tip">
                  <view class="group-tip-text">{{progress.lineUpMsg}}</view>
                  <view class="group-tip-triangle"></view>
                  <view class="group-tip-triangle-border"></view>
                </view>
              </block>
            </view>
          </block>
        </view>
      </block>
      <block wx:elif="{{orderStateDiagrams&&orderStateDiagrams.length==2}}">
        <view class="order-global-progress list2 clearfix r1bb">
          <block wx:for="{{orderStateDiagrams}}" wx:for-item="progress" wx:for-index="i"
            wx:key='orderStateDiagrams{{i}}'>
            <view class="global-progress-li progress-li{{i}}{{progress.nodeStatus?' on':''}}">
              <view class="global-bar-box">
                <view class="global-bar"></view>
                <view class="global-bar-ico"></view>
              </view>
              <block wx:if="{{progress.lineUpMsg}}">
                <view class="global-tip-box">
                  <view class="global-tip-text">{{progress.lineUpMsg}}</view>
                  <view class="global-triangle"></view>
                  <view class="global-triangle-border"></view>
                </view>
              </block>
              <block wx:if="{{progress.lineDownMsg}}">
                <view class="global-text">{{progress.lineDownMsg}}</view>
              </block>
            </view>
          </block>
        </view>
      </block>
      <block wx:elif="{{orderStateDiagrams&&orderStateDiagrams.length==3}}">
        <view class="order-global-progress list3 clearfix">
          <block wx:for="{{orderStateDiagrams}}" wx:for-item="progress" wx:for-index="i"
            wx:key='orderStateDiagrams{{i}}'>
            <view class="global-progress-li progress-li{{i}}{{progress.nodeStatus?' on':''}}">
              <view class="global-bar-box">
                <view class="global-bar"></view>
                <view class="global-bar-ico"></view>
              </view>
              <block wx:if="{{progress.lineUpMsg}}">
                <view class="global-tip-box">
                  <view class="global-tip-text">{{progress.lineUpMsg}}</view>
                  <view class="global-triangle"></view>
                  <view class="global-triangle-border"></view>
                </view>
              </block>
              <block wx:if="{{progress.lineDownMsg}}">
                <view class="global-text">{{progress.lineDownMsg}}</view>
              </block>
            </view>
          </block>
        </view>
      </block>
      <block wx:elif="{{orderStateDiagrams&&orderStateDiagrams.length>0&&isB2C!=1037&&isB2C!=1634&&isCardFlag!=1821}}">
        <view class="order-detail-progress list{{orderStateDiagrams.length}} clearfix">
          <block wx:for="{{orderStateDiagrams}}" wx:for-item="progress" wx:for-index="i"
            wx:key='orderStateDiagrams{{i}}'>
            <view class="progress-li {{progress.nodeStatus?'on':''}} progress-li{{i}}">
              <block wx:if="{{i<orderStateDiagrams.length-1}}">
                <view class="bar-box">
                  <view class="bar"></view>
                  <view class="bar-ico"></view>
                </view>
              </block>
              <block wx:if="{{progress.lineUpMsg}}">
                <view class="tip-box">
                  <text class="tip-text">{{progress.lineUpMsg}}</text>
                  <icon class="triangle"></icon>
                  <view class="triangle-border"></view>
                </view>
              </block>
              <block wx:if="{{progress.lineDownMsg}}">
                <text class="text">{{progress.lineDownMsg}}</text>
              </block>
              <block wx:if="{{progress.nodeDownMsg}}">
                <view class="progress-time">{{progress.nodeDownMsg}}</view>
              </block>
            </view>
          </block>
        </view>
      </block>
      <block
        wx:elif="{{orderStateDiagrams&&orderStateDiagrams.length>3&&(isB2C==1037||isB2C==1634||isCardFlag==1821)}}">
        <view class="order-global-progress list{{orderStateDiagrams.length}} clearfix">
          <block wx:for="{{orderStateDiagrams}}" wx:for-item="progress" wx:for-index="progressIndex"
            wx:key='orderStateDiagrams{{progressIndex}}'>
            <view class="global-progress-li progress-li{{progressIndex}}{{progress.nodeStatus?' on':''}}">
              <view class="global-bar-box">
                <view class="global-bar"></view>
                <view class="global-bar-ico"></view>
              </view>
              <block wx:if="{{progress.lineUpMsg}}">
                <view class="global-tip-box">
                  <view class="span global-tip-text">{{progress.lineUpMsg}}</view>
                  <view class="global-triangle"></view>
                  <view class="global-triangle-border"></view>
                </view>
              </block>
              <block wx:if="{{progress.lineDownMsg}}">
                <view class="global-text">{{progress.lineDownMsg}}</view>
              </block>
            </view>
          </block>
        </view>
      </block>
    </block>
    <block wx:if="{{(multipleShipmentsCounts>1||multipleShipmentsCounts==1&&deliverType==1)&&isB2C&&(isB2C==1037)}}">
      <view class="logistics-tip-more" catchtap="goLogisticsList">您有
        <view class="span">{{multipleShipmentsCounts}}</view>个包裹的物流信息，点击查看详情
      </view>
    </block>
    <block wx:if="{{isB2C==1634&&logisticsList.status}}">
      <view data-order-store-id="{{orderStoreId}}" class="a order-detail-logistics r1bt" catchtap="goViewLogistics">
        <view class="logistics-title">{{logisticsList.statusMsg}}</view>
        <view class="logistics-time">{{logisticsList.operateTimes}}</view>
      </view>
    </block>
    <block wx:else>
      <block
        wx:if="{{multipleShipmentsCounts==1&&deliverType==0&&isB2C&&(isB2C==1037)&&orderStatus!=51&&(deliverInfo||deliverTime)}}">
        <view data-order-store-id="{{orderStoreId}}" class="a order-detail-logistics r1bt" catchtap="goViewLogistics">
          <view class="logistics-title">{{deliverInfo}}</view>
          <view class="logistics-time">{{deliverTime}}</view>
        </view>
      </block>
    </block>

    <block wx:if="{{isB2C==1634}}">
      <block wx:if="{{shippingType==111}}">
        <view class="order-detail-user">
          <view class="name-phone clearfix">
            <view class="name f24">提货点：{{memberGroupAddressAddrTag}}</view>
          </view>
          <view class="address f24">提货点地址：{{addrFull}}</view>
          <block wx:if='{{addrMobile}}'>
            <view class='phone-self-box'>
              <view class='phone' catchtap='phoneSelf' data-phone='{{addrMobile}}'>联系提货点</view>
            </view>
          </block>
        </view>
      </block>
      <block wx:else>
        <view class="order-detail-user">
          <view class="name-phone clearfix">
            <view class="name">{{addrName}}</view>
            <view class="phone">{{addrMobile}}</view>
            <block wx:if="{{(shippingTypes[0]==1022||shippingTypes[0]==1023||shippingTypes[0]==1024)&&isB2C!=1634}}">
              <block wx:if="{{addrStete==1020||shippingTypes[0]==1024}}">
                <view class="ID-finished">已认证</view>
              </block>
              <block wx:else>
                <view class="ID-finished-none">未认证</view>
              </block>
            </block>
          </view>
          <view class="address">{{addrFull}}</view>
        </view>

      </block>
    </block>

    <block wx:else>
      <block wx:if='{{proType&&proType!=1&&proType!=1821}}'>
        <view class="order-detail-user">
          <view class="name-phone clearfix">
            <!-- <view class="name">提货人：{{addrName}}</view>
          <view class="phone">{{addrMobile}}</view> -->
            <view class="name f24">提货点：{{memberGroupAddressAddrTag}}</view>
          </view>
          <view class="address f24">提货点地址：{{addrFull}}</view>
          <!-- <block wx:if='{{addrMobile}}'>
            <view class='phone-self-box'>
              <view class='phone' catchtap='phoneSelf' data-phone='{{addrMobile}}'>联系提货点</view>
            </view>
          </block> -->
        </view>
      </block>
      <block
        wx:elif="{{isCardFlag!=1&&(shippingTypes[0]==110||shippingTypes[0]==112||shippingTypes[0]==1022||shippingTypes[0]==1023||shippingTypes[0]==1024)&&(addrName||addrMobile||addrFull)&&(offlineOrderType!=1182||offlineOrderType!=1184||offlineOrderType!=1122)}}">
        <view class="order-detail-user">
          <view class="name-phone clearfix">
            <view class="name">{{addrName}}</view>
            <view class="phone">{{addrMobile}}</view>
            <block wx:if="{{(shippingTypes[0]==1022||shippingTypes[0]==1023||shippingTypes[0]==1024)&&isB2C!=1634}}">
              <block wx:if="{{addrStete==1020||shippingTypes[0]==1024}}">
                <view class="ID-finished">已认证</view>
              </block>
              <block wx:else>
                <view class="ID-finished-none">未认证</view>
              </block>
            </block>
          </view>
          <view class="address">{{addrFull}}</view>
        </view>
      </block>
      <block wx:elif="{{goodsSence&&goodsSence>0}}">

      </block>
      <block
        wx:elif="{{isCardFlag==1709&&(shopName||shopAddr)||isCardFlag!=1&&(shippingTypes[0]==111||shippingTypes[0]==113)&&(shopName||shopAddr)&&(offlineOrderType!=1182||offlineOrderType!=1184||offlineOrderType!=1122)||(shopName||shopAddr)&&(offlineOrderType==1182)}}">
        <view class="order-detail-user">
          <view class="addr-shop">门店地址：{{shopName}}</view>
          <view class="address">{{shopAddr}}</view>

          <view class="order-cabinet-info" wx:if="{{cabinetAddrId>0}}">
            <view class="cabinet-address">提货点地址：{{cabinetAddress}}</view>
            <view class="cabinet-name" wx:if="{{cabinetName}}">提货点：{{cabinetName}} <block wx:if="{{cabinetDetailList}}"
                wx:for="{{cabinetDetailList}}">
                <block wx:if="{{index>0}}">,</block>{{item}}
              </block>
            </view>
            <view class="cabinet-tip" wx:if="{{cabinetName}}">请在自提柜上扫描订单码进行取货</view>
          </view>

        </view>

      </block>
    </block>


    <block wx:if="{{returnCount>0}}">
      <view data-return-count="{{returnCount}}" catchtap="goRefundListOrDetail" class="go-refund-record mt16 mb16">
        售后进度：您有
        <view class="span c-color">{{returnCount}}</view>条售后进度记录，点击查看详情
      </view>
    </block>
    <!--   <block wx:if="{{isCardFlag!=1&&goodsShippingType==113||foodShippingType==113}}">
            <view class="bg-caitiao"></view>
            <view class="tangshi-location clearfix">
                <view class="tangshi-location-l fl">堂食</view>
                <view class="tangshi-location-r fr">{{shopName}}</view>
            </view>
        </block>-->
    <block wx:for="{{storeGoodsList}}" wx:key="storeGoodsList{{j}}" wx:for-item="shop" wx:for-index="j">
      <view class="order-detail-list">
        <block wx:if="{{isCardFlag!=1}}">
          <view class="shop-name">
            <view class="shop-name-cnt">
              <!-- <block wx:if="{{shop.storeIcon}}">
                <image src="{{shop.storeIcon}}"></image>
              </block>
              <block wx:else>
                <image src="https://shgm.jjyyx.com/m/images/store_icon.png"></image>
              </block> -->
              <text>{{shop.storeName}}</text>
              <block wx:if="{{shop.storeType==1037}}">
                <image class="icon-global" src="https://shgm.jjyyx.com/m/images/icon_kexuan.png"></image>
              </block>
              <block wx:if="{{shop.storeType==1634}}">
                <image class="icon-global" src="https://shgm.jjyyx.com/m/images/icon_zhenpinkexuan.png"></image>
              </block>
            </view>
            <block wx:if="{{goodsSence==0&&isB2C!=1037&&isB2C!=1634&&payStatus==53&&orderStoreId}}">
              <view class="order-bar" catchtap="toUrlOrderStoreIdCode">订单条码</view>
            </block>
          </view>
        </block>
        <block
          wx:if="{{!(groupMode==1886&&groupState==1)&&isCardFlag!=1&&goodsSence==0&&(shop.shippingType==110||shop.shippingType==111||shop.shippingType==113||shop.shippingType==112)&&shippingTime&&offlineOrderType!=1184&&offlineOrderType!=1122&&canShowShippingTime||(shippingTime&&offlineOrderType==1182)&&canShowShippingTime}}">
          <view class="get-time clearfix r1bt">
            <view class="get-time-l">预约
              <block
                wx:if="{{(proType>0&&proType!=1&&proType!=1821)||proType==1178||proType==1888||offlineOrderType==1182}}">
                自提</block>
              <block wx:elif="{{shop.shippingType==110||shop.shippingType==112}}">配送</block>
              <block wx:elif="{{shop.shippingType==111}}">自提</block>
              <block wx:elif="{{shop.shippingType==113}}">堂食</block>
            </view>
            <view class="get-time-r">{{shippingTime}}</view>
          </view>
        </block>
        <view class="cart-item-list">
          <block wx:for="{{shop.goodsList}}" wx:for-item="product" wx:for-index="productIndex"
            wx:key="{{productIndex}}goodsList{{product.goodsId}}">
            <view class="li">
              <view class="wrap">
                <view class="cart-item">
                  <block wx:if="{{isCardFlag==999||isCardFlag==1821||proType==1888||groupMode}}">
                    <view class="icon-pintuan"></view>
                  </block>
                  <view class="item-img">
                    <block wx:if="{{product.foodStatus==1}}">
                      <view class="already-served">已出餐</view>
                    </block>
                    <block wx:elif="{{product.foodStatus==2}}">
                      <view class="already-served">已取餐</view>
                    </block>
                    <block wx:if="{{product.isGift==1&&product.giftOutStockFlag&&product.giftOutStockFlag==1}}">
                      <view class="masker-present">已赠完</view>
                    </block>
                    <block wx:elif="{{product.returnStatus!=0}}">
                      <view class="masker-refunded">申请退款</view>
                    </block>
                    <image src="{{product.goodsImage}}"></image>
                  </view>
                  <view class="item-info weigh-goods">
                    <view class="h3">
                      <block wx:if="{{product.isGift==1}}">
                        <view class="em">赠品</view>
                      </block>
                      <block wx:elif="{{product.isGift==2}}">
                        <view class="em">换购</view>
                      </block>
                      <block wx:elif="{{product.isGift==998||product.isGift==1640}}">
                        <view class="em">限时抢</view>
                      </block>
                      {{product.goodsName}}
                    </view>
                    <view class="h4 color-999">
                      <block wx:if="{{product.materielType==30}}">
                        <view class="h4-view">
                          <text class="property-out-hiding">{{product.materielExtPropertyStr}}</text>
                        </view>
                      </block>
                      <block wx:if="{{product.goodsSpec&&isB2C!=1037&&isB2C!=1634}}">
                        <view class="h4-view">规格：{{product.goodsSpec}}
                          <block wx:if="{{product.pricingMethod==391&&offlineOrderType==0}}">
                            <view class="icon-info" catchtap="weightPop"></view>
                          </block>
                        </view>
                      </block>
                      <view class="h4-view">
                        <block wx:if="{{product.pricingMethod==391}}">重量：{{product.weightStr}}</block>
                        <block wx:else>数量：{{product.goodsNum}}</block>
                        <view class="div-space3">\t</view>单价：￥
                        <block wx:if="{{product.isGift&&product.isGift==3}}">{{product.memberPriceStr}}</block>
                        <block wx:else>{{product.goodsPriceStr}}</block>
                        <block wx:if="{{product.goodsUnit}}">/{{product.goodsUnit}}</block>
                        <block
                          wx:if="{{(product.memberPriceStr!=product.goodsOriginPriceStr)&&product.isMember||product.goodsPriceStr!=product.goodsOriginPriceStr}}">
                          <view class="detail-del"> ￥{{product.goodsOriginPriceStr}}</view>
                        </block>
                      </view>
                    </view>
                    <block wx:if="{{1||product.isGift!=1}}">
                      <view class="price">
                        <block wx:if="{{product.isGift==3&&0}}">
                          <view class="member-tag">会员价</view>
                        </block>
                        <view class="em">{{product.goodsAmountStr}}</view>
                      </view>
                    </block>
                  </view>
                </view>
              </view>
              <!-- offlineOrderType==1184地推的不能售后 -->
              <block wx:if="{{(offlineOrderType==1182||offlineOrderType==1122||goodsSence>0)}}">
                <block
                  wx:if="{{(product.isGift!=1&&product.isGift!=2)&&product.isReturnFlag==1&&isReturnFlag==1&&(product.goodsNum-product.returnCount)>=0&&(offlineOrderType==1182&&checkFlag==1||offlineOrderType==1122||goodsSence>0)}}">
                  <block wx:if="{{isPromotionGoodsFlag==1&&returnStatus!=1}}">
                    <view class="refund-box">
                      <view class="refund-cnt clearfix">
                        <view class="refund-btn{{product.goodsNum==product.returnCount?' color-999':''}}"
                          catchtap="phoneService" data-servicePhone="{{servicePhone}}">申请退货</view>
                      </view>
                    </view>
                  </block>
                  <block wx:else>
                    <view class="refund-box">
                      <view class="refund-cnt clearfix">
                        <view catchtap="goodsToRefund" data-order-item-id="{{product.orderItemId}}"
                          data-return-count="{{product.returnCount}}" data-goods-num="{{product.goodsNum}}"
                          data-return-status="{{product.returnStatus}}" data-shop-id="{{shop.shopId}}"
                          data-is-gift="{{product.isGift}}" data-store-id="{{shop.storeId}}"
                          data-sku-id="{{product.goodsSkuId}}" data-goods-sence="{{goodsSence}}"
                          data-offline-order-type="{{offlineOrderType}}"
                          class="refund-btn{{product.goodsNum==product.returnCount?' color-999':''}}">申请退货</view>
                      </view>
                    </view>
                  </block>
                </block>

              </block>
              <block
                wx:elif="{{(isB2C==0||isB2C==1||isB2C==1037||isB2C==1634)&&product.isReturnFlag==1&&(product.isGift!=1&&product.isGift!=2)&&isReturnFlag==1&&(product.goodsNum-product.returnCount)>=0&&(orderStatus==46||orderStatus==1088||orderStatus==952)&&offlineOrderType!=1184}}">
                <block wx:if="{{isPromotionGoodsFlag==1&&returnStatus!=1}}">
                  <view class="refund-box">
                    <view class="refund-cnt clearfix">
                      <view class="refund-btn{{product.goodsNum==product.returnCount?' color-999':''}}"
