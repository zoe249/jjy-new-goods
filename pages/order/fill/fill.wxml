            <view class='li'>{{lessGoodsList.goodsName}}</view>
          </block>
        </block>
      </view>
    </block>
    <block wx:elif="{{maskBox == 4}}">
      <view class='slot-default-list'>
        <block wx:if="{{outStockSkuNameList}}">
