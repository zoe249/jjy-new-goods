page{
  background: #f8f8f8;
}
.home-container{
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100vh;
  padding-bottom: 112rpx;
}
.theme {
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
  width: 100%;
}
.page-top-bg{
  position: absolute;
  width: 100%;
  height: 400rpx;
  background: var(--red);
  z-index: -1;
  border-bottom-left-radius: 48rpx;
  border-bottom-right-radius: 48rpx;
}
.logas{
  width: 176rpx;
}
.logas .loga-txt{
  color: #FFFCFC;
  font-size: 32rpx;
  line-height: 64rpx;
}
.logas .loga-img{
  width: 176rpx;
  height: 64rpx;
}

/* 头部 自提点-搜索*/
.header {
  position: sticky;
  top: 0;
  display: flex;
  width: 100%;
  align-items: center;
  padding: 16rpx 190rpx 16rpx 32rpx;
  color: #fff;
  z-index: 1024;
  overflow: hidden;
  background: var(--red);
}
.header .hd-bg{
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: -1;
}
.c-not-theme {
  color: #fff;
}

.not-theme {
  color: #0F3F69;
}

.channel-wrap.not-theme {
  color: #94969C;
}

.shop-info {
  display: flex;
  font-size: 24rpx;
  align-items: center;
  margin: 0 32rpx;
  color: #fff;
  background: rgba(255, 255, 255, 0.16);
  padding: 10rpx 16rpx 10rpx 8rpx;
  border-radius: 8rpx;
}
.shop-info-wrap{
  flex: 1;
  display: flex;
  min-width: 206rpx;
  align-items: center;
}
.shop-info image {
  width: 48rpx;
  height: 48rpx;
  border-radius: 100%;
  background: rgba(255, 255, 255, 0.8) url('https://shgm.jjyyx.com/m/images/icon-default.png') no-repeat center;
  background-size: 48rpx;
  box-shadow: 0rpx 3rpx 6rpx rgba(223, 223, 223, 0.16);
}

.shop-info .shar-home{
  display: flex;
  background: none;
  border: 0;
  color: #fff;
  padding: 0;
  line-height: 32rpx;
  font-size: 24rpx;
  margin-left: auto;
  margin-right: 0;
  width: 90rpx;
}
.shop-info .shar-home::after{
  border: 0;
}
.shop-info .shar-home text{
  display: block;
  width: 90rpx;
  white-space:nowrap;
}

.search-btn {
  width: 304rpx;
  display: flex;
  justify-content: center;
  flex-shrink:1;
  align-items: center;
  height: 64rpx;
  background: #fff;
  padding: 0 16rpx;
  border-radius: 64rpx;
  transition: 0.3s;
  margin-left: auto;
  transition: all 0.4s;
  margin-right: 32rpx;
}

.search-btn view {
  color: var(--gray);
  font-size: 24rpx;
  padding-left: 15rpx;
  border-radius: 4rpx;
}

.search-btn text {
  display: block;
  width: 34rpx;
  height: 34rpx;
  font-size: 36rpx;
  color: var(--gray);
}


.channel-tabs {
  padding: 0 32rpx;
}

.channel-wrap {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #fff;
}

.iconClass {
  position: absolute;
  right: 0;
  top: 22rpx;
  font-size: 38rpx;
}

.channel-tabs .c-tab {
  width: 100%;
  flex: 1;
  padding-right: 48rpx;
}
/* 广告 */
.banner {
  position: relative;
  padding-top: 16rpx;
}

.banner .dic {
  position: absolute;
  right: 48rpx;
  bottom: 12rpx;
  width: 64rpx;
  height: 36rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  background: rgba(0, 0, 0, 1);
  opacity: 0.4;
  border-radius: 32rpx;
  letter-spacing: 4rpx;
}

.banner swiper {
  height: 304rpx;
  /* margin-top: 16rpx; */
}

.banner swiper-item {
  box-sizing: border-box;
}

.banner .img {
  height: 304rpx;
  width: 686rpx;
  background: #fff;
  border-radius: 16rpx;
  background: url("https://shgm.jjyyx.com/m/images/icon_g_banner.png") no-repeat center;
  background-size: 686rpx 304rpx;
  margin: 0 auto;
  overflow: hidden;
}
.homesolt{
  width: 100%;
  display: block;
  margin: 8rpx 0 0;
}
.seat-box{
  margin-top: 16rpx;
}
.seat-box image{
  height: 32rpx;
}
/* flash-sale */
.flash-sale{
  width: 686rpx;
  /* height: 272rpx; */
  margin: 16rpx auto 0;
  display: flex;
  flex-direction: column;
  background: #fff;
  padding: 16rpx 10rpx 0;
  border-radius: 24rpx;
  overflow: hidden;
}
.flash-sale-timer{
  display: flex;
  align-items: center;
  font-size: 28rpx;
  color: #0C0C21;
  font-weight: bold;
  margin-bottom: 8rpx;
  padding: 0 8rpx;
  position: relative;
}
.flash-titimg{
  width: 672rpx;
  height: 64rpx;
}
.flash-sale-timer .time-wrap{
  color: #FF2D63;
  /* margin-left: 10rpx; */
  position: absolute;
  left: 146rpx;
}
.flash-sale-timer .time-wrap .time{
  display: inline-block;
  width:36rpx;
  height:36rpx;
  line-height: 36rpx;
  text-align: center;
  background: #FF2D63;
  border-radius:20rpx;
  color: #fff;
  font-size: 24rpx;
  margin: 0 6rpx;
}

.flash-sale-list{
  display: flex;
  background: linear-gradient(180deg, rgba(255, 45, 99, 0.06) 0%, rgba(255, 255, 255, 0.1) 80%);
  border-radius: 20rpx;
  padding: 16rpx;
}
.flash-sale-goods {
  width: 148rpx;
  padding: 8rpx 0 0;
  margin-right: 16rpx;
}
.flash-sale-goods:last-child{
  margin-right: 0;
}
.flash-sale-goods .img{
  width: 144rpx;
  height: 144rpx;
  background: #fff;
  border-radius: 16rpx;
}
.flash-sale-goods .price{
  width: 100%;
  padding: 0 6rpx;
  font-size:24rpx;
  font-weight:bold;
  line-height:64rpx;
  height: 64rpx;
  color:#373737;
  text-align: center;
  white-space:nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.flash-sale-goods .btn{
  width: 100%;
  font-size: 24rpx;
  line-height: 1.6;
  color: #FFFFFF;
  background: #EC414D;
  border-radius: 20rpx;
  text-align: center;
}
.flash-sale-goods.not-stock .btn{
  background: var(--bgGrayWhite);
  color: var(--gray);
}
/* TAB */
.scroll-nav-container{
  /* height: 100rpx; */
  /* margin-top: 32rpx; */
  position: sticky;
  top: 0;
  z-index: 2;
  background: #f8f8f8;
}
.scroll-nav{
  width: 100%;
  white-space: nowrap;
  z-index: 2;
  /* padding-top: 32rpx; */
}
.scroll-nav .scroll-nav-wrap{
  padding-left: 24rpx;
  height: 84rpx;
  background: #f8f8f8;
}
.scroll-nav .scroll-nav-wrap view {
  display: inline-block;
  position: relative;
  padding: 32rpx 20rpx;
  color: #373737;
  font-size: 28rpx;
  letter-spacing: 2rpx;
  font-weight: bold;
  transition: all 0.1s;
  opacity: .6;
}

.scroll-nav .scroll-nav-wrap view.scroll-nav-act {
  font-size: 28rpx;
  font-weight: bold;
  color: #0F3F69;
  opacity: 1;
}
