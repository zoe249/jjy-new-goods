/* 活动板块 */
.plate {
  display: flex;
  justify-content: space-between;
  width: 686rpx;
  border-radius: 24rpx;
  margin: 16rpx auto 0;
  /* background: #fff; */
}

.plate .new,
.plate .hot {
  width: 336rpx;
  background:#fff;
  border-radius: 8rpx;
}

.plate-title {
  display: flex;
  align-items: center;
  font-size: 28rpx;
  font-weight: bold;
  color: var(--black);
  padding: 16rpx 16rpx 8rpx;
}
.plate-title .user-icon{
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  margin-right: -10rpx;
  border: 1px solid #fff;
}
.user-flex{
  display: flex;
  margin-left: 12rpx;
}
.user-flex .plate-txt{
  font-size: 24rpx;
  color: #DCDEE2;
  margin-left: 24rpx;
}
.new .plate-icon {
  display: inline-block;
  width: 48rpx;
  height: 32rpx;
  background: url(http://m.lyzlcloud.com/m/images/yunguo/icon_home_new.png) no-repeat center;
  background-size: 48rpx 32rpx;
  margin-left: 8rpx;
}

.hot .plate-icon {
  display: inline-block;
  width: 32rpx;
  height: 32rpx;
  background: url(http://m.lyzlcloud.com/m/images/yunguo/icon_home_hot.png) no-repeat center;
  background-size: 32rpx auto;
  margin-left: 8rpx;
}

.plate-flex {
  display: flex;
  align-items: center;
  /* justify-content: space-between; */
  padding: 0 16rpx 16rpx;
  text-align: center;
  height: 106rpx;
}
.plate-flex .plate-item{
  margin-right: 8rpx;
}
.plate-flex .plate-item:nth-child(3){
  margin-right: 0;
}
.not-list {
  width: 100%;
  text-align: center;
  font-size: 24rpx;
  color: var(--grey);
}

.plate-item .image {
  width: 96rpx;
  height: 96rpx;
  border-radius: 8rpx;
  /* background: url('http://m.lyzlcloud.com/m/images/yunguo/detail_goods_s.png') no-repeat center; */
  background-size: cover;
  background: #f4f4f4;
}