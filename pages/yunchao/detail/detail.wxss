page {
  /* color: #777; */
  background: #fff;
}

.hidden {
  display: none !important;
}

.clearfix:after {
  clear: both;
}

.clearfix {
  zoom: 1;
}

.clearfix:before, .clearfix:after {
  content: "";
  display: block;
  overflow: hidden;
  visibility: hidden;
  width: 0;
  height: 0;
}

.wrapper {
  height: 100vh;
  padding-bottom: 112rpx;
}

.group-rule-des {
  text-align: center;
  line-height: 48rpx;
  color: #12284e;
  font-size: 24rpx;
  background-color: #e6e9ed;
}

.back-home {
  background: #fff;
  z-index: 99;
  position: fixed;
  bottom: 200rpx;
  right: 30rpx;
  width: 96rpx;
  height: 96rpx;
  background: url("https://shgm.jjyyx.com/m/images/detail/icon-back-home.png?t=20200707") no-repeat center;
  background-size: 100% 100%;
  text-indent: -9999rpx;
}

/* 帮帮团头部 */

.help-group-head {
  padding: 0 32rpx;
}

.help-group-head .help-title-box {
  position: relative;
}

.help-group-head .help-title-box .help-title {
  font-size: 24rpx;
  color: #94969c;
}

.help-group-head .help-title-price {
  font-size: 32rpx;
  color: #FF2D63;
  line-height: 44rpx;
  display: inline-block;
}

.help-group-head .help-title-people {
  display: inline-block;
}

.help-group-head .help-already {
  font-size: 24rpx;
  color: #777;
  line-height: 44rpx;
  position: absolute;
  right: 0;
  top: 4rpx;
}

.help-group-head .help-salePrice {
  padding: 20rpx 0;
  font-size: 24rpx;
  color: #94969c;
  line-height: 32rpx;
  text-decoration: line-through;
}

.help-group-head .help-tip {
  border-top: 1rpx solid #e7e7e7;
  min-height: 88rpx;
  position: relative;
  padding: 26rpx 0 26rpx 85rpx;
  font-size: 24rpx;
  line-height: 32rpx;
  color: #777;
}

.help-group-head .help-tip .help-tip-title {
  position: absolute;
  top: 24rpx;
  left: 0;
  color: #ffa601;
  border: 1rpx solid #ffa601;
  line-height: 32rpx;
  font-size: 24rpx;
  padding: 0 8rpx;
}

/* 抽奖团头部信息 */

.draw-head {
  padding: 0 0 20rpx;
}

.draw-tip-box {
  padding: 0 32rpx;
}

.draw-tip {
  border-top: 1rpx solid #e7e7e7;
  min-height: 88rpx;
  position: relative;
  padding: 26rpx 0 26rpx 85rpx;
  font-size: 24rpx;
  line-height: 32rpx;
