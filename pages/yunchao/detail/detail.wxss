/* 帮帮团规则玩法 */

.goods-promotion-container.help-promotion-container .group-header-container {
  padding: 0 32rpx;
}

.help-promotion-container .help-head {
  position: relative;
}

.help-promotion-container .help-head-left {
  height: 86rpx;
  font-size: 30rpx;
  line-height: 42rpx;
  color: #313031;
  padding: 22rpx 0;
}

.help-promotion-container .help-head-info {
  position: absolute;
  right: 0;
  top: 0;
  height: 85rpx;
  line-height: 85rpx;
  color: #94969c;
  font-size: 24rpx;
  padding-right: 27rpx;
}

.help-promotion-container .help-head-info::after {
  content: '';
  position: absolute;
  right: 0rpx;
  top: 50%;
  width: 16rpx;
  height: 16rpx;
  border-left: 2rpx solid #9E9EA6;
  border-bottom: 2rpx solid #9E9EA6;
  -webkit-transform: translateY(-50%) scaleY(1) rotateZ(-135deg);
  transform: translateY(-50%) scaleY(1) rotateZ(-135deg);
}

.help-promotion-container .group-help-des {
  border-top: 1rpx solid #e7e7e7;
  color: #313031;
  font-size: 24rpx;
  line-height: 96rpx;
}

