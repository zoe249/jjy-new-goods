.container {
	overflow-x: hidden
}

.empty-container a {
	width: 10rem;
	height: 2.1875rem;
	line-height: 2.1875rem;
	background: #fff;
	border: 1px solid #ff5d3d;
	color: #ff5d3d;
	margin-top: 1.625rem
}

.effective,invalid {
	width: 100%;
	height: 100vh;
	overflow-y: auto
}

.card-wrap {
	width: 100%;
	height: 100%;
	position: relative
}

.card-wrap ul {
	padding: .9375rem 0;
	width: 21.4375rem;
	margin: 0 auto
}

.card-wrap li {
	display: -webkit-box;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-ms-flex-align: center;
	align-items: center;
	height: 5.625rem;
	border-radius: .625rem;
	background: url("../images/bg_kabao.png") no-repeat center;
	margin-bottom: .9375rem;
	color: #fff
}

.card-wrap li .item-icon {
	width: 5.625rem;
	height: 5.625rem
}

.card-wrap li .item-icon img {
	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	-ms-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
	width: 2.5rem;
	height: 2.5rem;
	border-radius: 2.5rem;
	overflow: hidden
}

.card-wrap li .info {
	width: 16.4375rem;
	font-size: .9375rem;
	line-height: 1.25rem
}

.card-wrap li .info h4 {
	height: 2.5rem;
	line-height: 1.25rem;
	overflow: hidden;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical
}

.card-wrap li .info p {
	width: 100%;
	height: 1.25rem;
	line-height: 1.25rem
}

.card-wrap li .info span {
	font-size: .75rem;
	height: 1.6875rem;
	display: table-cell;
	vertical-align: bottom
}

.card-wrap li .info span em {
	font-size: 1.6875rem
}

.card-wrap li .info span em.infinite {
	font-size: 1.25rem
}

.card-wrap li .will-invalid {
	right: .625rem;
	top: 0;
	width: 3.3125rem;
	height: 2.375rem;
	background: url("../images/icon_kb_guoqi.png") no-repeat center;
	background-size: 3.3125rem
}

.card-wrap .effective ul {
	padding-bottom: 5rem
}

.card-wrap .invalid {
	position: fixed;
	top: 100%;
	left: 0;
	width: 100%;
	height: 100%;
	margin-top: -2.8125rem;
	overflow-y: auto;
	background: #fff
}

.card-wrap .invalid h3 {
	height: 2.25rem;
	line-height: 2.25rem;
	text-align: center;
	font-size: .875rem;
	color: #999
}

.card-wrap .invalid h3 span {
	display: inline-block;
	width: .625rem;
	height: .59375rem;
	background: url("../images/icon_shanghua.png") no-repeat center;
	background-size: .625rem .59375rem
}

.card-wrap .invalid li {
	background: url("../images/bg_kabao_shixiao.png") no-repeat center
}

.card-wrap .invalid li h4 {
	height: auto
}

.card-wrap .invalid li .item-icon img {
	-webkit-filter: grayscale(100%);
	filter: grayscale(100%);
	-webkit-filter: gray;
	filter: gray
}

.card-wrap .invalid.slideOutUp {
	top: 0;
	margin-top: 0;
	-webkit-transition: all .6s;
	-o-transition: all .6s;
	transition: all .6s
}

.card-wrap .invalid.slideOutUp h3 span {
	background: url("../images/icon_xiahua.png") no-repeat center;
	background-size: .625rem .59375rem
}

.card-wrap .invalid.slideOutDown {
	-webkit-transition: all .6s;
	-o-transition: all .6s;
	transition: all .6s
}

@-webkit-keyframes slideOutUp {
	from {
		-webkit-transform: translate3d(0, 0, 0);
		transform: translate3d(0, 0, 0)
	}

	to {
		visibility: hidden;
		-webkit-transform: translate3d(0, -100%, 0);
		transform: translate3d(0, -100%, 0)
	}
}

@keyframes slideOutUp {
	from {
		-webkit-transform: translate3d(0, 0, 0);
		transform: translate3d(0, 0, 0)
	}

	to {
		visibility: hidden;
		-webkit-transform: translate3d(0, -100%, 0);
		transform: translate3d(0, -100%, 0)
	}
}

.slideOutUp {
	-webkit-animation-name: slideOutUp;
	animation-name: slideOutUp
}

@-webkit-keyframes slideOutDown {
	from {
		-webkit-transform: translate3d(0, 0, 0);
		transform: translate3d(0, 0, 0)
	}

	to {
		visibility: hidden;
		-webkit-transform: translate3d(0, 100%, 0);
		transform: translate3d(0, 100%, 0)
	}
}

@keyframes slideOutDown {
	from {
		-webkit-transform: translate3d(0, 0, 0);
		transform: translate3d(0, 0, 0)
	}

	to {
		visibility: hidden;
		-webkit-transform: translate3d(0, 100%, 0);
		transform: translate3d(0, 100%, 0)
	}
}

.slideOutDown {
	-webkit-animation-name: slideOutDown;
	animation-name: slideOutDown
}

.card-details-h-a {
	height: auto !important
}

.card-details-wrap {
	height: 100vh;
	padding: .9375rem
}

.card-details-wrap .card-top {
	position: relative;
	height: 9.375rem;
	background: url("../images/bg_kabao_details.png") no-repeat center;
	border-radius: .625rem
}

.card-details-wrap .card-top .card-cover-info {
	display: table-cell;
	vertical-align: middle;
	height: 9.375rem;
	padding: 1.25rem;
	color: #fff
}

.card-details-wrap .card-top .card-cover-info img {
	display: inline-block;
	vertical-align: middle;
	width: 4.5rem;
	height: 4.5rem;
	border-radius: 100%;
	overflow: hidden
}

.card-details-wrap .card-top .card-cover-info p {
	position: absolute;
	left: 7rem;
	top: 50%;
	padding-right: .625rem;
	font-size: 1rem;
	-webkit-transform: translateY(-50%);
	-ms-transform: translateY(-50%);
	transform: translateY(-50%);
	overflow: hidden;
	-o-text-overflow: ellipsis;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical
}

.card-details-wrap .invalid-card {
	left: 50%;
	top: 10.3125rem;
	bottom: .9375rem;
	-webkit-transform: translateX(-50%);
	-ms-transform: translateX(-50%);
	transform: translateX(-50%)
}

.card-details-wrap .info {
	background: #fff;
	width: 20rem;
	margin: 0 auto;
	border-bottom-left-radius: .625rem;
	border-bottom-right-radius: .625rem
}

.card-details-wrap .info h3 {
	height: 2.8125rem;
	line-height: 2.8125rem;
	text-align: center;
	font-size: 1.0625rem
}

.card-details-wrap .info ul {
	padding: 0 1.25rem;
	font-size: .875rem;
	background: #fff;
	border-bottom-left-radius: .625rem;
	border-bottom-right-radius: .625rem
}

.card-details-wrap .info ul li {
	height: 2.8125rem;
	line-height: 2.8125rem;
	border-bottom: 1px solid #c0c0c0
}

.card-details-wrap .info ul li a {
	display: block
}

.card-details-wrap .info ul li span {
	color: #999999
}

.card-details-wrap .info ul li span.forword {
	padding-right: 1.25rem
}

.forword:after {
	position: absolute;
	right: .125rem;
	top: 50%;
	width: .5rem;
	height: .5rem;
	content: "";
	border-left: .0625rem solid #999;
	border-bottom: .0625rem solid #999;
	-webkit-transform: translateY(-50%) scaleY(1) rotateZ(-135deg);
	-ms-transform: translateY(-50%) scaleY(1) rotate(-135deg);
	transform: translateY(-50%) scaleY(1) rotateZ(-135deg)
}

.available-card-code {
	position: relative;
	width: 20rem;
	padding: 3.75rem 0 5rem;
	margin: 0 auto;
	border-bottom-left-radius: .625rem;
	border-bottom-right-radius: .625rem
}

.available-card-code #qr_code {
	width: 9.375rem;
	height: 9.375rem;
	margin: 0 auto
}

.available-card-code .number {
	font-size: .875rem;
	text-align: center;
	margin-top: .9375rem
}

.available-card-code .card-tips {
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 1.875rem;
	line-height: 1.875rem;
	text-align: center;
	background: #fffbf3;
	color: #ffaa00;
	border-bottom-left-radius: .625rem;
	border-bottom-right-radius: .625rem
}

.available-card.info ul li:last-child {
	border: 0
}

.available-card.info {
	margin-top: .9375rem;
	border-radius: .625rem
}