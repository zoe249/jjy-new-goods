.banner{
  width:100%;
  position: relative;
}
.noRightBr{
  border-right:none !important;
}
.noBottomBr{
  border-bottom:none !important;
}
.noLeftBr{
  border-left:none !important;
}
.noTopBr{
  border-top:none !important;
}
#box{
  min-height:100vh;
}
.cart-ico{
  position:fixed;
  z-index:99;
  bottom:180rpx;
  left:0rpx;
  width:114rpx;
  height:114rpx;
  background: url("https://shgm.jjyyx.com/m/images/icon-fixed-cart.png") center no-repeat;
  background-size: 100%;
}
.cart-ico .cart-num{
  position:absolute;
  right:0;
  top:0;
  padding:0 10rpx;
  border-radius:16rpx;
  color:#fff;
  font-size:24rpx;
  line-height:32rpx; 
  background-color: #FF4752;
}
.banner image{
  display: block;
  width:100%;
  height:auto;
}
.coupon-box{
  position: relative;
  padding:16rpx;
  background-color:rgba(0,0,0,0.6);
  margin:-160rpx 30rpx 0;
  border-radius: 16rpx;
}
.coupon-box .item{
  height:128rpx;
  background-color:#D4B871;
  border-radius: 8rpx;
  position: relative;
  padding:0 0 0 208rpx;
  color:#313031;
  margin-bottom:16rpx;
}
.coupon-box .item:last-child{
  margin-bottom:0rpx;
}
.coupon-box .item.off{
  background-color: #E7E7E7;
  color:#94969C;
}
.coupon-box .item .item-right{
  position:absolute;
  top:6rpx;
  left:0;
  width:184rpx;
  height:116rpx;
  text-align: center;
  border-right:1rpx solid #313031;
}
.coupon-box .item .item-right .price{
  font-size:42rpx;
  line-height:60rpx;
  font-weight: bold;
  padding:10rpx 0 0;
}
.coupon-box .item .item-right .small{
  font-size:28rpx;
}
.coupon-box .item .item-right .des{
  font-size:24rpx;
}
.coupon-box .item .item-right .text{
  font-size:36rpx;
  line-height:116rpx
}
.coupon-box .item .item-left .text1{
  font-size:30rpx;
  line-height:42rpx;
  padding:6rpx 0 0;
  white-space: nowrap;
  text-overflow: ellipsis;
  height:48rpx;
  overflow: hidden;
}
.coupon-box .item .item-left .text2{
  font-size:24rpx;
  line-height:32rpx;
  padding:8rpx 0 0;
  white-space: nowrap;
  text-overflow: ellipsis;
  height:40rpx;
  overflow: hidden;
}
.coupon-box .item .item-left .text3{
  font-size:18rpx;
  line-height:24rpx;
  padding:10rpx 0 0;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.coupon-box .item .coupon-btn{
  position: absolute;
  right:16rpx;
  top:32rpx;
  line-height: 64rpx;
  border-radius: 32rpx;
  box-shadow: 0 0 4rpx rgba(0,0,0,0.2);
  background-color: #fff;
  width:128rpx;
  text-align: center;
  font-size:26rpx;
  font-weight: bold;
  color:#94969C;
}
.coupon-box .item .coupon-btn.canReceive{
  color:#D4B871;
}
.goods-box{
  padding:0 0 50rpx;
}
.goods-box .goods-title{
  min-height:0rpx;
}
.goods-box .goods-title image{
  width:100%;
  height:0;
}
.goods-box .ul{
  background-color: #fff;
  border-radius: 16rpx;
  margin:0 15rpx;
  display: -webkit-flex;
  display:flex;
  flex-wrap:wrap;
}

.one-goods-li{
  padding:32rpx 32rpx 0;
  width:100%;
}
.one-goods-li .li{
