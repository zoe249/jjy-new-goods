  align-items:flex-end;
}
.one-goods-li .li .goods-cnt .goods-price .price{
  color:#FF4752;
  font-size: 32rpx;
  line-height: 40rpx;
}
.one-goods-li .li .goods-cnt .goods-price .unit{
  color:#94969C;
  font-size:24rpx;
  line-height: 38rpx;
}
.one-goods-li .li .goods-cnt .btn{
  position:absolute;
  right:0;
  bottom:0;
  width:48rpx;
  height:48rpx;
  text-indent:-9999rpx;
  background: url("https://shgm.jjyyx.com/m/images/icon_add_shaow.png") no-repeat center;
  background-size: 100%;
}
.one-goods-li .li .goods-cnt .btn-text{
