/* 自定义封装css部分开始 */
/* 点击穿透 */
.click_true {
	pointer-events: none;
}

.click_false {
	pointer-events: auto;
}

/* 盒阴影 */
.hyy {
	box-shadow: 0 0 50rpx 0 rgba(0, 0, 0, 0.08);
}

/* 换行 */
.hh {
	word-break: break-all;
	overflow-wrap: break-word;
}

/* 超出省略的样式 */
.slh_one {
	display: block;
	text-overflow: ellipsis;
	white-space: nowrap;
	overflow: hidden;
}

.slh_two {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
}

.slh_three {
	overflow: hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 3;
	-webkit-box-orient: vertical;
}

/* 怪异盒模型 */
.box {
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

