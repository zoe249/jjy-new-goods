/* 顶部红包弹出框部分 */
#header .Tooltip {
	width: 750rpx;
	min-height: 182rpx;
	background: #FFFFFF;
	padding: 29rpx 30rpx;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 100;
}
#header .Tooltip .left{
	width: 65rpx;
	height: 65rpx;
	margin-right: 23rpx;
}
#header .Tooltip .left image{
	width: 100%;
	height: 100%;
}
#header .Tooltip .right .top{
	font-size: 31rpx;
	font-family: PingFang;
	font-weight: 400;
	color: #FD7968;
	line-height: 10rpx;
	margin-bottom: 20rpx;
}
#header .Tooltip .right .bottom{
	width: 549rpx;
	font-size: 23rpx;
	font-family: PingFang;
	font-weight: 400;
	color: #7D7D7D;
	line-height: 32rpx;
}

@keyframes nextshow {
	from {
		transform: translateY(-860rpx);
	}

	to {
		transform: translateY(0rpx);
	}
}

@keyframes nexthide {
	from {
		transform: translateY(0rpx);
	}

	to {
		transform: translateY(-900rpx);
	}
}

#header .hide {
	animation: nexthide;
	animation-duration: 2s;
	animation-iteration-count: 1;
	animation-fill-mode: forwards;
}

#header .show {
	animation: nextshow;
	animation-duration: 0.5s;
	animation-iteration-count: 1;
	animation-fill-mode: forwards;
}

/* 内容区域 */
#section {
	position: fixed;
	top: 0;
	left: 0;
	width: 100vw;
	height: 100vh;
	z-index: 2;
}

/* 背景图 */
#section .bg {
	position: absolute;
	top: 0;
