.user-hy {
    position: relative;
    display: inline-block;
    margin-top:8rpx;
    padding: 0 0 0 45rpx;
    height: 50rpx;
    line-height: 50rpx;
    color: #fefefe;
    background: #60ccf1;
    -webkit-background-size: 0.75rem 0.75rem;
    background-size: 0.75rem 0.75rem;
    border-radius: 1.5625rem;
}
.user-hy image{
    position: absolute;
    left: -7rpx;
    top: -9rpx;
    width: 72rpx;
    height: 72rpx;
    z-index: 1;
}
.user-hy button {
    height: 50rpx;
    line-height: 50rpx;
    background: transparent;
    color: #fefefe;
    padding-left: 30rpx;
    font-size: 18rpx;
    vertical-align: bottom;
}
.user-hy button::after{
    border: 0;
}
.user-hy text{
    display: inline;
    font-size: 30rpx;
    margin-right: 10rpx;
}
button[type="warn"] {
    color:#FFFFFF;
    background-color:#ff5d3b;

}
.head-nav .c-color:before{
    content:'ï¿¥';
