}
.order-state-list .cell{
    padding:32rpx 0 !important;
}
.order-state-list text{
    display: block;
}
.order-state-list .order-num{
    position: absolute;
    left:52%;
    top:20rpx;
    display: inline-block;
    height: 0;
    line-height: 0;
    padding:14rpx 8rpx;
