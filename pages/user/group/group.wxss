    transform: translateY(-50%);
}
.head-nav-title{
    margin: 0 0 0 32rpx;
    height: 96rpx;
    line-height: 96rpx;
}
.head-nav {
    height: 160rpx;
    display: flex;
    flex-wrap: wrap;
    text-align: center;
    font-size:32rpx;
    z-index: 2;
    -webkit-box-pack:center;
    -webkit-justify-content:center;
    -webkit-box-align: center;
    -webkit-align-items: center;
    box-pack:center;
    justify-content:center;
    box-align: center;
    align-items: center;
    align-items: center;
}
.head-nav .item-col2 {
    flex: 1;
    height: 160rpx;
    max-width: 50%;
    font-size:32rpx;
    padding: 48rpx 0 40rpx;
}
.head-nav .item-col2 .text{
    display: block;
    font-size:24rpx;
}
.head-nav::after{
    content:'';
    position: absolute;
    left:0;
    bottom:0;
    width:100%;
    height: 100%;
    background:#fff;
    opacity: 0.6;
    z-index: -1;
}
.head-nav .c-color{
    padding-left:24rpx;
}
.ui-flex {
    display: -webkit-box !important;
    display: -webkit-flex !important;
    display: flex !important;
    -webkit-flex-wrap: wrap;
    -ms-flex-wrap: wrap;
    flex-wrap: wrap;
    -webkit-box-pack:center;
    -webkit-justify-content:center;
    -webkit-box-align: center;
    -webkit-align-items: center;
    box-pack:center;
    justify-content:center;
    box-align: center;
    align-items: center;
    -webkit-box-sizing: border-box;
    box-sizing: border-box;
}
.ui-flex .cell {
    position: relative;
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
    width: 0;
    -webkit-flex-basis: 0;
    flex-basis: 0;
    max-width: 100%;
    display: block;
    padding: 0 !important;
}
.my-item2 .item2-title {
    position: relative;
    margin: 0 0 0 32rpx;
    height: 96rpx;
